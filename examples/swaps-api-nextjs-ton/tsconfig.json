{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "typescript-config/nextjs.json",
  "compilerOptions": {
    "rootDir": ".",
    "baseUrl": "./src",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    "src",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
